<form action="PlanServlet" method="post" class="note">
	<i class="fas fa-times-circle cross"></i> 
	<label for="" class="note-head">Planning</label> 
	<label for="title"class="input-tag">Title</label> 
	<input type="text" name="title"class="input-fields title-tag"> 
	<label for="desc" class="input-tag">Description</label>
	<textarea name="content" id="desc" cols="30" rows="10"
		class="input-fields"></textarea>
	<button type="submit" class="submit">submit</button>
</form>